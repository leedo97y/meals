{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "initdb.js",
    "lib/meals.js",
    "app/layout.jsx",
    "app/page.jsx",
    "app/community/page.jsx",
    "app/meals/page.jsx",
    "app/meals/[mealSlug]/page.jsx",
    "app/meals/share/page.jsx",
    "components/images-slide/image-slides.jsx",
    "components/main-header/nav-link.jsx",
    "components/main-header/header.jsx",
    "components/main-header/header-background.jsx",
    "components/meals/meals-item.jsx",
    "components/meals/meals-grid.jsx"
  ],
  "exclude": ["node_modules"]
}
